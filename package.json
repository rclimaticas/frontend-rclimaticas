{
  "name": "nextarter-tailwind",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "turbo": "pnpm dlx turbo@2.0.9",
    "build:turbo": "pnpm turbo build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "check:turbo": "pnpm turbo lint type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.428.0",
    "next": "^14.2.5",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.5.0",
    "next-themes": "^0.3.0",
    "pnpm": "^9.10.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "^5.3.0",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.4.0",
    "@tailwindcss/typography": "^0.5.14",
    "@types/react": "18.3.2",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-sznm": "^2.0.3",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.41",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.10",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=20.14.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.2"
}
